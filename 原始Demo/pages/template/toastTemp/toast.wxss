.toastTemp{
    width:100%;
    height: 100%;
    position: fixed;
    top:0;
    left: 0;
    background: rgba(0,0,0,.5);
    display: none;
}
.toastTemp.show{
    display: block;
}
.toastTemp .toast-wrap{
    display: inline-block;
    background: #ffffff;
    border-radius: 10px;
    padding:10rpx 0;
    position: absolute;
    top:50%;
    left:50%;
    transform: translate(-50%,-50%);
    font-family: "微软雅黑";
}
.toastTemp .toast-content{
    line-height: 80rpx;
    box-sizing: border-box;
    border-bottom:1px solid lawngreen;
    text-align: center;
    font-size:34rpx;
    font-weight: bold;
    padding:0 40rpx;
}
.toastTemp .toast-btn{
    line-height: 80rpx; 
     text-align: center;
    font-size:34rpx;
    color: green;
}
